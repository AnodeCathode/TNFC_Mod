{
    "multipart": [
        {   "apply": { "model": "quark:paper_wall_post_sakura" }},
        {   "when": { "north": true },
            "apply": { "model": "quark:paper_wall_side_sakura" }
        },
        {   "when": { "east": true },
            "apply": { "model": "quark:paper_wall_side_sakura", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "quark:paper_wall_side_alt_sakura" }
        },
        {   "when": { "west": true },
            "apply": { "model": "quark:paper_wall_side_alt_sakura", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "quark:paper_wall_noside_sakura" }
        },
        {   "when": { "east": false },
            "apply": { "model": "quark:paper_wall_noside_alt_sakura" }
        },
        {   "when": { "south": false },
            "apply": { "model": "quark:paper_wall_noside_alt_sakura", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "quark:paper_wall_noside_sakura", "y": 270 }
        }
    ]
}
